{
  "name": "geewallet",
  "description": "GeeWallet SDK for Node.js.",
  "version": "1.1.0",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./worker": {
      "types": "./types/worker.d.ts",
      "import": "./dist/esm/worker.js",
      "require": "./dist/cjs/worker.cjs",
      "default": "./dist/esm/worker.js"
    }
  },
  "files": [
    "dist",
    "types",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsdown",
    "prepublishOnly": "npm run lint && npm run build",
    "prepare": "husky"
  },
  "author": "nswbmw",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nswbmw/geewallet-node.git"
  },
  "keywords": [
    "geewallet",
    "geepay",
    "payment",
    "credit card",
    "visa",
    "mastercard"
  ],
  "devDependencies": {
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "eslint": "9.39.2",
    "husky": "9.1.7",
    "neostandard": "0.12.2",
    "tsdown": "^0.20.0-beta.3"
  },
  "dependencies": {
    "https-proxy-agent": "7.0.6",
    "lite-request": "1.0.2",
    "socks-proxy-agent": "8.0.5",
    "sort-keys": "6.0.0"
  }
}
